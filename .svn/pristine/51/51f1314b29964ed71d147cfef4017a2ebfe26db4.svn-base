package net.iclassmate.bxyd.bean.message;

/**
 * 用于发送命令消息
 * Created by xyd on 2016/9/13.
 */
public class User {
    private String id;
    private String name;
    private String icon;

    public User(String id, String name, String icon) {
        this.id = id;
        this.name = name;
        this.icon = icon;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getIcon() {
        return icon;
    }

    public void setIcon(String icon) {
        this.icon = icon;
    }
}
