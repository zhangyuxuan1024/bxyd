<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f9"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="#393f4f">

        <ImageView
            android:id="@+id/owner_information_iv_back"
            android:layout_width="10dp"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:src="@mipmap/ic_fanhui" />

        <TextView
            android:id="@+id/owner_information_tv_back"
            android:layout_width="50dp"
            android:layout_height="40.5dp"
            android:layout_centerVertical="true"
            android:gravity="center|left"
            android:layout_marginLeft="6.5dp"
            android:layout_toRightOf="@+id/owner_information_iv_back"
            android:text="我的"
            android:textColor="@color/white"
            android:textSize="14sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="个人资料"
            android:textColor="@color/white"
            android:textSize="17sp" />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <RelativeLayout
        android:id="@+id/owner_information_rl_head"
        android:layout_width="match_parent"
        android:layout_height="69.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="头像"
            android:textColor="#202025"
            android:textSize="15sp" />

        <net.iclassmate.xydspace.view.CircleImageView
            android:id="@+id/owner_information_iv_icon"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp"
            android:background="#00000000"
            android:src="@mipmap/ic_gaitouxiang" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="用户号"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/owner_information_tv_userCode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:id="@+id/owner_information_rl_name"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="姓名"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/owner_information_tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:id="@+id/owner_information_rl_2code"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector"
        android:visibility="visible">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="我的二维码"
            android:textColor="#202025"
            android:textSize="15sp" />

        <ImageView
            android:layout_width="13.5dp"
            android:layout_height="13.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp"
            android:src="@mipmap/ic_erweima" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:id="@+id/owner_information_rl_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/setting_pressed_selector"
        android:paddingBottom="15dp"
        android:paddingTop="15dp">

        <TextView
            android:id="@+id/owner_information_tv_descriptionleft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="简介"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/owner_information_tv_description"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="65dp"
            android:layout_marginRight="35dp"
            android:layout_toLeftOf="@+id/owner_information_iv_descriptionright"
            android:layout_toRightOf="@+id/owner_information_tv_descriptionleft"
            android:gravity="right"
            android:maxLength="255" />

        <ImageView
            android:id="@+id/owner_information_iv_descriptionright"
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="15dp" />

    <RelativeLayout
        android:id="@+id/owner_information_rl_area"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="地区"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/owner_information_tv_area"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:id="@+id/owner_information_rl_birthday"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="生日"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/owner_information_tv_birthday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:id="@+id/owner_information_rl_sex"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="性别"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/owner_information_tv_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:id="@+id/vMasker"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/red"
        android:visibility="gone" />
</LinearLayout>