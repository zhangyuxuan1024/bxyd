<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f9"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="#393f4f">
        <ImageView
            android:id="@+id/jigouinformation_iv_back"
            android:layout_width="10dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp"
            android:src="@mipmap/ic_fanhui"/>
        <TextView
            android:id="@+id/jigouinformation_tv_back"
            android:layout_width="wrap_content"
            android:layout_height="20.5dp"
            android:text="我的"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:layout_toRightOf="@+id/jigouinformation_iv_back"
            android:layout_centerVertical="true"
            android:layout_marginLeft="6.5dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="机构资料"
            android:textColor="@color/white"
            android:textSize="17sp"
            android:layout_centerInParent="true"/>

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <RelativeLayout
        android:id="@+id/jigouinformation_rl_head"
        android:layout_width="match_parent"
        android:layout_height="69.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="头像"
            android:textColor="#202025"
            android:textSize="15sp" />

        <net.iclassmate.xydspace.view.CircleImageView
            android:id="@+id/jigouinformation_iv_icon"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp"
            android:src="@mipmap/ic_jigou_guanzhu" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="机构号"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/jigouinformation_tv_userCode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:id="@+id/jigouinformation_rl_name"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="机构名称"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/jigouinformation_tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:id="@+id/jigouinformation_rl_2code"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="机构二维码"
            android:textColor="#202025"
            android:textSize="15sp" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="15dp" />

    <RelativeLayout
        android:id="@+id/jigouinformation_rl_area"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="地区"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/jigouinformation_tv_area"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp" />

    <RelativeLayout
        android:id="@+id/jigouinformation_rl_pro"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="行业"
            android:textColor="#202025"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/jigouinformation_tv_pro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8.5dp"
            android:src="@mipmap/ic_next" />
    </RelativeLayout>

    <View
        android:id="@+id/vMasker"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/red"
        android:visibility="gone" />




</LinearLayout>