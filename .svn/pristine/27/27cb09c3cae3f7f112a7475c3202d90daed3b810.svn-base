<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f9">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="#393f4f">
        <ImageView
            android:id="@+id/owner_setting_iv_back"
            android:layout_width="10dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp"
            android:src="@mipmap/ic_fanhui"/>
        <TextView
            android:id="@+id/owner_setting_tv_back"
            android:layout_width="wrap_content"
            android:layout_height="20.5dp"
            android:text="我的"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:layout_toRightOf="@+id/owner_setting_iv_back"
            android:layout_centerVertical="true"
            android:layout_marginLeft="6.5dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:textColor="@color/white"
            android:textSize="17sp"
            android:layout_centerInParent="true"/>

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <RelativeLayout
        android:id="@+id/setting_rl_msg"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="消息提醒"
            android:textSize="15sp"
            android:textColor="#202025"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp" />
        <ToggleButton
            android:id="@+id/setting_tb_message"
            android:layout_width="38dp"
            android:layout_height="20dp"
            android:background="@android:color/transparent"
            android:button="@drawable/switch_btn"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp"
        android:visibility="gone"/>
    <RelativeLayout
        android:id="@+id/setting_rl_changepassword"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="修改密码"
            android:textSize="15sp"
            android:textColor="#202025"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp" />
        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:src="@mipmap/ic_next"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp"/>
    <RelativeLayout
        android:id="@+id/setting_rl_bangphone"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="绑定手机号"
            android:textSize="15sp"
            android:textColor="#202025"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp" />
        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:src="@mipmap/ic_next"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp"/>
    <RelativeLayout
        android:id="@+id/setting_rl_privacy"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="隐私设置"
            android:textSize="15sp"
            android:textColor="#202025"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp" />
        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:src="@mipmap/ic_next"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp"/>
    <RelativeLayout
        android:id="@+id/setting_rl_update"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="版本更新"
            android:textSize="15sp"
            android:textColor="#202025"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp" />
        <FrameLayout
            android:layout_width="41dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp">

            <ImageView
                android:id="@+id/setting_iv_next"
                android:layout_width="15.5dp"
                android:layout_height="15.5dp"
                android:src="@mipmap/ic_next"
                android:layout_gravity="right"/>
            <ImageView
                android:id="@+id/setting_iv_new"
                android:layout_width="41dp"
                android:layout_height="20dp"
                android:src="@mipmap/ic_new"
                android:visibility="invisible"/>
        </FrameLayout>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp"
        />
    <RelativeLayout
        android:id="@+id/setting_rl_aboutus"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于我们"
            android:textSize="15sp"
            android:textColor="#202025"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp" />
        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:src="@mipmap/ic_next"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.8dp"/>
    <RelativeLayout
        android:id="@+id/setting_rl_feedback"
        android:layout_width="match_parent"
        android:layout_height="44.5dp"
        android:background="@drawable/setting_pressed_selector">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="意见反馈"
            android:textSize="15sp"
            android:textColor="#202025"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp" />
        <ImageView
            android:layout_width="15.5dp"
            android:layout_height="15.5dp"
            android:src="@mipmap/ic_next"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/setting_rl_tuichuzhanghao"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/setting_iv_tuichuzhanghao"
            android:layout_width="345dp"
            android:layout_height="45dp"
            android:src="@drawable/tuichuzhanghao_pressed_selector"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="41.5dp"/>
    </RelativeLayout>

</LinearLayout>